 */
"))

(defun insert-include-config ()
  (interactive)
  (insert
"
#ifdef HAVE_CONFIG_H
#include \"config.h\"
#endif

"))

(defun insert-license-and-include-config ()
  (interactive)
  (insert-license)
  (insert-include-config))

